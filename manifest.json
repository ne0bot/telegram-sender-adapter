{
  "author": "Michael Stegeman",
  "description": "Simple telegram sending actions. Needs to be configured with chatid and token.",
  "gateway_specific_settings": {
    "webthings": {
      "exec": "{nodeLoader} {path}",
      "primary_type": "notifier",
      "strict_max_version": "*",
      "strict_min_version": "0.10.0"
    }
  },
  "homepage_url": "https://github.com/mrstegeman/telegram-sender-adapter",
  "id": "telegram-sender-adapter",
  "license": "MPL-2.0",
  "manifest_version": 1,
  "name": "Telegram",
  "options": {
    "default": {
      "token": "",
      "chatid": ""
    },
    "schema": {
      "type": "object",
      "required": [
        "token",
        "chatid"
      ],
      "properties": {
        "token": {
          "type": "string"
        },
        "chatid": {
          "type": "string"
        }
      }
    }
  },
  "short_name": "Telegram",
  "version": "0.0.8"
}
